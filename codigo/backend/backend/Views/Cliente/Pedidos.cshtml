@model IEnumerable<backend.Models.Produto>

@{
    ViewData["Title"] = "Pedidos";
    Layout = "~/Views/Shared/_LayoutCliente.cshtml";
}



<head>
    <style>
        .containerImagem {
            width: 100%;
            display: flex;
            align-items: center;
            justify-content: center;
            margin: 0;
            padding: 0;
        }

        .imagemApresentation {
            background-size: cover;
            width: 100%;
            display: block;
        }

        .image-text {
            display: flex;
            align-items: center;
            justify-content: center;
            font-size: 8vw;
            text-align: center;
            position: absolute;
            color: aliceblue;
            font-weight: 700;
        }
    </style>
</head>


<body>
    <nav class="containerImagem">
        <img class="imagemApresentation" src="~/assets/ToWebdesign/BkgOrangeBlock.svg" />
        <p class="image-text">Pedidos</p>
    </nav>
</body>

